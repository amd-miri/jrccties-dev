<?php

/**
 * @file
 * Install file.
 */

 /**
  * Implements hook_enable().
  */
function jrc_expand_replace_enable() {
  drupal_set_message(t('The JRC expand replace feature is enabled.'), $type = 'status');
}

/**
 * Implements hook_disable().
 */
function jrc_expand_replace_disable() {
  drupal_set_message(t('The JRC expand replace feature is disabled.'), $type = 'status');
}
