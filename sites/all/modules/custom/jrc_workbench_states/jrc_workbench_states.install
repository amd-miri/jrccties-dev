 * Install file.
 */
 
 /**
  * Implement hook_enable()
  */
function jrc_workbench_states_enable() {
  drupal_set_message(t('The JRC Workbench states module is enabled.'), $type = 'status');
}

 /**
  * Implement hook_disable()
  */
function jrc_workbench_states_disable() {
  drupal_set_message(t('The JRC Workbench states module is disabled.'), $type = 'status');
}