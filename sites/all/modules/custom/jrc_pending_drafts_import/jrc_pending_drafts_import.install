<?php

/**
 * @file
 * Install file.
 */

 /**
  * Implements hook_enable().
  */
function jrc_pending_drafts_import_enable() {
  drupal_set_message(t('The JRC pending drafts migration module is enabled. You can now run the import.'), $type = 'status');
}

/**
 * Implements hook_disable().
 */
function jrc_pending_drafts_import_disable() {
  drupal_set_message(t('The JRC pending drafts migration module is disabled.'), $type = 'status');
}
